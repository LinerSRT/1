        custom:dial="@drawable/xy_01_beijing"
        custom:hand_hour="@drawable/xy_01_shizhen"
        custom:hand_minute="@drawable/xy_01_fenzhen"
        custom:hand_second="@drawable/xy_01_miaozhen"/>

</RelativeLayout>