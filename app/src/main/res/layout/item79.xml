<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.wiiteck.clockpreviewer"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

   <com.wiiteck.clockpreviewer.Watch_qw_12
        android:id="@+id/watchtable"
        custom:hand_hour="@drawable/watch_qw_13_shizhen"
		custom:hand_minute="@drawable/watch_qw_13_fenzhen"
		custom:hand_second="@drawable/watch_qw_13_miaozhen"
		custom:dial="@drawable/watch_qw_13_beijing"
		custom:center="@drawable/watch_qw_13_center"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent" 
		android:layout_marginTop="0px"
		android:visibility="visible" />
       
</RelativeLayout>
